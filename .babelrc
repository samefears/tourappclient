{
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"],
    }
  },
  "plugins": [
    "transform-runtime",
    "transform-class-properties",
    [
      "transform-async-to-generator",
      {
        "polyfill": false,
        "regenerator": true,
      }
    ]
  ]
}
